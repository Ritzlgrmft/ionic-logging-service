{
	"name": "ionic-logging-service-app",
	"version": "0.0.0-development",
	"description": "Test app for ionic-logging-service",
	"author": {
		"name": "Markus Wagner",
		"email": "markus.wagner02@web.de"
	},
	"repository": "https://github.com/Ritzlgrmft/ionic-logging-service.git",
	"license": "MIT",
	"private": false,
	"keywords": [
		"ionic",
		"angular",
		"mobile",
		"logging",
		"log4javascript"
	],
	"scripts": {
		"build": "ng build ionic-logging-service --prod && cpx 'projects/ionic-logging-service/README.md' dist/ionic-logging-service --verbose",
		"test": "npm run test-coverage && npm run test-compodoc && ng lint",
		"test-coverage": "ng test ionic-logging-service --code-coverage --watch false --browsers ChromeHeadless",
		"test-compodoc": "cd projects/ionic-logging-service && compodoc --coverageTest 100 && cd ../..",
		"test-watch": "ng test ionic-logging-service --code-coverage",
		"lint": "ng lint",
		"codecov": "codecov",
		"compodoc": "cd projects/ionic-logging-service && compodoc && cd ../..",
		"semantic-release": "semantic-release"
	},
	"dependencies": {
		"@angular/animations": "^7.2.15",
		"@angular/common": "^7.2.15",
		"@angular/compiler": "^7.2.15",
		"@angular/core": "^7.2.15",
		"@angular/forms": "^7.2.15",
		"@angular/http": "^7.2.15",
		"@angular/platform-browser": "^7.2.15",
		"@angular/platform-browser-dynamic": "^7.2.15",
		"@angular/router": "^7.2.15",
		"core-js": "^2.5.4",
		"log4javascript": "^1.4.15",
		"rxjs": "~6.2.0",
		"zone.js": "~0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "0.13.9",
		"@angular-devkit/build-ng-packagr": "0.13.9",
		"@angular/cli": "7.3.8",
		"@angular/compiler-cli": "^7.2.15",
		"@angular/language-service": "7.2.15",
		"@compodoc/compodoc": "1.1.10",
		"@types/jasmine": "2.8.16",
		"@types/jasminewd2": "2.0.6",
		"@types/node": "12.7.1",
		"codecov": "3.5.0",
		"codelyzer": "5.1.0",
		"cpx": "1.5.0",
		"jasmine-core": "2.99.1",
		"jasmine-spec-reporter": "4.2.1",
		"karma": "~3.1.4",
		"karma-chrome-launcher": "2.2.0",
		"karma-coverage-istanbul-reporter": "2.0.5",
		"karma-jasmine": "~1.1.2",
		"karma-jasmine-html-reporter": "^0.2.2",
		"karma-junit-reporter": "1.2.0",
		"ng-packagr": "4.7.1",
		"protractor": "5.4.2",
		"puppeteer": "1.15.0",
		"semantic-release": "15.13.19",
		"ts-node": "7.0.1",
		"tsickle": "0.34.0",
		"tslib": "1.10.0",
		"tslint": "5.18.0",
		"tslint-angular": "3.0.2",
		"typescript": "3.1.6"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"release": {
		"pkgRoot": "dist/ionic-logging-service"
	}
}
