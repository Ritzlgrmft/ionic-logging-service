{
	"name": "ionic-logging",
	"version": "0.0.0-development",
	"description": "Components for Ionic logging",
	"author": {
		"name": "Markus Wagner",
		"email": "markus.wagner02@web.de"
	},
	"repository": "https://github.com/Ritzlgrmft/ionic-logging-service.git",
	"license": "MIT",
	"private": false,
	"keywords": [
		"ionic",
		"angular",
		"mobile",
		"logging",
		"log4javascript"
	],
	"scripts": {
		"build": "npm run build-service && npm run build-viewer && npm run build-viewer-app",
		"build-service": "ng build ionic-logging-service --prod && cpx 'projects/ionic-logging-service/README.md' dist/ionic-logging-service --verbose",
		"build-viewer": "ng build ionic-logging-viewer --prod && cpx 'projects/ionic-logging-viewer/README.md' dist/ionic-logging-viewer --verbose",
		"build-viewer-app": "ng build ionic-logging-viewer-app --prod",
		"test": "npm run test-service && npm run test-viewer && ng lint && npm run e2e",
		"test-service": "npm run test-coverage-service && npm run test-compodoc-service",
		"test-viewer": "npm run test-coverage-viewer && npm run test-compodoc-viewer",
		"test-coverage-service": "ng test ionic-logging-service --code-coverage --watch false --browsers ChromeHeadless",
		"test-coverage-viewer": "ng test ionic-logging-viewer --code-coverage --watch false --browsers ChromeHeadless",
		"test-compodoc-service": "cd projects/ionic-logging-service && compodoc --coverageTest 100 && cd ../..",
		"test-compodoc-viewer": "cd projects/ionic-logging-viewer && compodoc --coverageTest 100 && cd ../..",
		"test-watch-service": "ng test ionic-logging-service --code-coverage",
		"test-watch-viewer": "ng test ionic-logging-viewer --code-coverage",
		"lint": "ng lint",
		"e2e": "ng e2e --project ionic-logging-viewer-app-e2e",
		"codecov": "codecov",
		"compodoc": "npm run compodoc-service && npm run compodoc-viewer",
		"compodoc-service": "cd projects/ionic-logging-service && compodoc && cd ../..",
		"compodoc-viewer": "cd projects/ionic-logging-viewer && compodoc && cd ../..",
		"semantic-release": "npm run semantic-release-service && npm run semantic-release-viewer",
		"semantic-release-service": "semantic-release --pkgRoot dist/ionic-logging-service",
		"semantic-release-viewer": "semantic-release --pkgRoot dist/ionic-logging-viewer"
	},
	"dependencies": {
		"@angular/animations": "^7.2.15",
		"@angular/common": "^7.2.15",
		"@angular/compiler": "^7.2.15",
		"@angular/core": "^7.2.15",
		"@angular/forms": "^7.2.15",
		"@angular/http": "^7.2.15",
		"@angular/platform-browser": "^7.2.15",
		"@angular/platform-browser-dynamic": "^7.2.15",
		"@angular/router": "^7.2.15",
		"@ionic/angular": "^4.0.0",
		"core-js": "^2.5.4",
		"ionicons": "^4.0.0",
		"log4javascript": "^1.4.15",
		"rxjs": "~6.2.0",
		"zone.js": "~0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "0.13.9",
		"@angular-devkit/build-ng-packagr": "0.13.9",
		"@angular/cli": "7.3.8",
		"@angular/compiler-cli": "^7.2.15",
		"@angular/language-service": "7.2.15",
		"@compodoc/compodoc": "1.1.10",
		"@types/jasmine": "2.8.16",
		"@types/jasminewd2": "2.0.6",
		"@types/node": "12.7.1",
		"codecov": "3.5.0",
		"codelyzer": "5.1.0",
		"cpx": "1.5.0",
		"jasmine-core": "2.99.1",
		"jasmine-reporters": "2.3.2",
		"jasmine-spec-reporter": "4.2.1",
		"karma": "~3.1.4",
		"karma-chrome-launcher": "2.2.0",
		"karma-coverage-istanbul-reporter": "2.0.5",
		"karma-jasmine": "~1.1.2",
		"karma-jasmine-html-reporter": "^0.2.2",
		"karma-junit-reporter": "1.2.0",
		"ng-packagr": "4.7.1",
		"protractor": "5.4.2",
		"puppeteer": "1.15.0",
		"semantic-release": "15.13.19",
		"semantic-release-ado": "1.2.0",
		"ts-node": "7.0.1",
		"tsickle": "0.34.0",
		"tslib": "1.10.0",
		"tslint": "5.18.0",
		"tslint-angular": "3.0.2",
		"typescript": "3.1.6"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	}
}
