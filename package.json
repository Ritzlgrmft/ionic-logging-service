{
  "name": "ionic-logging",
  "version": "12.0.0",
  "description": "Components for Ionic logging",
  "author": {
    "name": "Markus Wagner",
    "email": "markus.wagner02@web.de"
  },
  "repository": "https://github.com/Ritzlgrmft/ionic-logging-service.git",
  "license": "MIT",
  "private": false,
  "keywords": [
    "ionic",
    "angular",
    "mobile",
    "logging",
    "log4javascript"
  ],
  "scripts": {
    "build": "npm run build-service && npm run build-viewer && npm run build-viewer-app",
    "build-service": "ng build ionic-logging-service --prod && cpx 'projects/ionic-logging-service/README.md' dist/ionic-logging-service --verbose",
    "build-viewer": "ng build ionic-logging-viewer --prod && cpx 'projects/ionic-logging-viewer/README.md' dist/ionic-logging-viewer --verbose",
    "build-viewer-app": "ng build ionic-logging-viewer-app --prod",
    "test": "npm run test-service && npm run test-viewer && ng lint",
    "test-service": "npm run test-coverage-service && npm run test-compodoc-service",
    "test-viewer": "npm run test-coverage-viewer && npm run test-compodoc-viewer",
    "test-coverage-service": "ng test ionic-logging-service --code-coverage --watch false --browsers ChromeHeadless",
    "test-coverage-viewer": "ng test ionic-logging-viewer --code-coverage --watch false --browsers ChromeHeadless",
    "test-compodoc-service": "cd projects/ionic-logging-service && compodoc --coverageTest 100 && cd ../..",
    "test-compodoc-viewer": "cd projects/ionic-logging-viewer && compodoc --coverageTest 100 && cd ../..",
    "test-watch-service": "ng test ionic-logging-service --code-coverage",
    "test-watch-viewer": "ng test ionic-logging-viewer --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e --project ionic-logging-viewer-app-e2e",
    "codecov": "codecov",
    "compodoc": "npm run compodoc-service && npm run compodoc-viewer",
    "compodoc-service": "cd projects/ionic-logging-service && compodoc && cd ../..",
    "compodoc-viewer": "cd projects/ionic-logging-viewer && compodoc && cd ../..",
    "semantic-release-version": "semantic-release --plugins @semantic-release/git --plugins @semantic-release/commit-analyzer --plugins semantic-release-ado --debug",
    "semantic-release-service": "semantic-release --pkgRoot dist/ionic-logging-service",
    "postinstall": "ngcc"
  },
  "dependencies": {
    "@angular/common": "^11.0.0",
    "@angular/compiler": "^11.0.0",
    "@angular/core": "^11.0.0",
    "@angular/forms": "^11.0.0",
    "@angular/platform-browser": "^11.0.0",
    "@angular/platform-browser-dynamic": "^11.0.0",
    "@angular/router": "^11.0.0",
    "@ionic/angular": "^5.0.0",
    "ionicons": "^5.0.0",
    "rxjs": "~6.5.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1102.6",
    "@angular-devkit/build-angular": "^0.1102.6",
    "@angular-devkit/schematics": "^11.0.0",
    "@angular/cli": "^11.0.0",
    "@angular/compiler-cli": "^11.0.0",
    "@angular/language-service": "^11.0.0",
    "@compodoc/compodoc": "1.1.11",
    "@ionic/angular-toolkit": "3.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "13.9.5",
    "codecov": "3.7.1",
    "codelyzer": "^6.0.0",
    "cpx": "1.5.0",
    "fsevents": "1.2.9",
    "jasmine-core": "~3.6.0",
    "jasmine-reporters": "2.3.2",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "2.0.1",
    "ng-packagr": "^11.2.4",
    "protractor": "~7.0.0",
    "puppeteer": "^6.0.0",
    "ts-node": "8.6.2",
    "tslint": "~6.1.0",
    "tslint-angular": "3.0.3",
    "typescript": "4.0.7",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
